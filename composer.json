{
    "name": "alecgunnar/httprouter",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alec Carpenter",
            "email": "alecgunnar@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "guzzlehttp/psr7": "~1.3.0"
    },
    "autoload": {
        "psr-4": {
            "AlecGunnar\\HttpRouter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlecGunnar\\HttpRouter\\Test\\": "tests/src/"
        }
    },
    "suggest": {
        "guzzlehttp/psr7": "Implementations of the various PSR-7 interfaces."
    }
}
